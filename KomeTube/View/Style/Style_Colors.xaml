    <SolidColorBrush x:Key="VotingCenter.Title.Background" Color="#FF252526">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VotingCenter.Title.Foreground" Color="#FFFFFFEE">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VotingCenter.Title.Background.Translucent" Color="#AFFFFFEE">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VoteCandidate.Candidate.Background" Color="#FF3e3e42">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VoteCandidate.Candidate.Background.Translucent" Color="#AFF0E0D6">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VoteCandidate.Candidate.Foreground" Color="#FFFFFFEE">
    </SolidColorBrush>
    <SolidColorBrush x:Key="VoteCandidate.SlideTextItem.Foreground" Color="#BEBEBE">
    </SolidColorBrush>

    <SolidColorBrush x:Key="PuzzleCenter.AnswerList.Background" Color="#FF252526">
    </SolidColorBrush>
    <SolidColorBrush x:Key="PuzzleCenter.AnswerList.Foreground" Color="#FFFFFFEE">
    </SolidColorBrush>
    <SolidColorBrush x:Key="PuzzleAnswer.Answer.Background" Color="#FF3e3e42">
