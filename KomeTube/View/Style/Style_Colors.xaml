<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:KomeTube.View.Style">

    <SolidColorBrush x:Key="Main.Window.Background" Color="#FF252526">
    </SolidColorBrush>

    <SolidColorBrush x:Key="Main.DataGrid.Background" Color="#FF252526">
    </SolidColorBrush>
    <SolidColorBrush x:Key="Main.DataGrid.Foreground" Color="#FFFFFFEE">
    </SolidColorBrush>
    <SolidColorBrush x:Key="Main.DataGrid.Row.Selected" Color="#FF3e3e42">
