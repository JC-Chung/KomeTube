<TextBox x:Class="KomeTube.View.Component.NumericTextBox"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
         xmlns:local="clr-namespace:KomeTube.View.Component"
         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
         mc:Ignorable="d"
         HorizontalAlignment="Left"
         VerticalAlignment="Top"
         d:DesignHeight="300"
         d:DesignWidth="300"
         DataObject.Pasting="PastingHandler"
         GotKeyboardFocus="On_GotKeyboardFocus"
         GotMouseCapture="On_GotMouseCapture"
         LostFocus="On_LostFocus"
         PreviewTextInput="OnPreviewTextInput"
         TextChanged="OnTextChanged">
</TextBox>